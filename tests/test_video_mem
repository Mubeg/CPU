jmp main

put: /put(int c, int pos)
pop dx
pop ax
pop bx
push ax
push [bx]
push dx
return
end


main:
push 1
pop
push 0
pop cx
cycle_init:
	push 120
	push cx
	jne if
	push 49
	pop ax
	jmp end_if
if:
	push 48
	pop ax
end_if:
	push cx
	push ax
	call put
	push 1
	push cx
	add
	pop cx
	push 1620
	push cx
	jb cycle_init

push 0
pop cx
cycle:
	draw
	push 1
	push cx
	add
	pop cx
	push 1000s
	push cx
	jb cycle

end
